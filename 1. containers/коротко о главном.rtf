\par
interface Enumeration \{\par
    // \u1045?\u1089?\u1090?\u1100? \u1083?\u1080? \u1077?\u1097?\u1077? \u1101?\u1083?\u1077?\u1084?\u1077?\u1085?\u1090?\u1099??\par
    public boolean hasMoreElements();\par
    // \u1042?\u1079?\u1103?\u1090?\u1100? \u1089?\u1083?\u1077?\u1076?\u1091?\u1102?\u1097?\u1080?\u1081? \u1101?\u1083?\u1077?\u1084?\u1077?\u1085?\u1090?.\par
    public Object nextElement(); \par
\}\par
// \u1056?\u1077?\u1072?\u1083?\u1080?\u1079?\u1072?\u1094?\u1080?\u1103? \u1087?\u1077?\u1088?\u1077?\u1095?\u1080?\u1089?\u1083?\u1077?\u1085?\u1080?\u1103? \u1076?\u1083?\u1103? \u1088?\u1072?\u1073?\u1086?\u1090?\u1099? \u1089? \u1094?\u1077?\u1083?\u1099?\u1084?\u1080? \u1095?\u1080?\u1089?\u1083?\u1072?\u1084?\u1080?.\par
class Enum implements Enumeration \{\par
    // \u1042?\u1077?\u1082?\u1090?\u1086?\u1088?, \u1074? \u1082?\u1086?\u1090?\u1086?\u1088?\u1086?\u1084? \u1093?\u1088?\u1072?\u1085?\u1103?\u1090?\u1089?\u1103? \u1095?\u1080?\u1089?\u1083?\u1072?.\par
    private int[] values;\par
    // \u1050?\u1086?\u1083?\u1080?\u1095?\u1077?\u1089?\u1090?\u1074?\u1086? \u1091?\u1078?\u1077? \u1080?\u1079?\u1074?\u1083?\u1077?\u1095?\u1077?\u1085?\u1085?\u1099?\u1093? \u1095?\u1080?\u1089?\u1077?\u1083?.\par
    private int   current;\par
\par
    // \u1050?\u1086?\u1085?\u1089?\u1090?\u1088?\u1091?\u1082?\u1090?\u1086?\u1088?.\par
    public Enum(int[] values) \{\par
        this.values = values;\par
        current = 0;\par
    \}\par
    public boolean hasMoreElements() \{\par
        return current < values.length;\par
    \}\par
    public Object nextElement() \{\par
        return new Integer(values[current++]);\par
    \}\par
    // \u1058?\u1077?\u1089?\u1090?\u1086?\u1074?\u1072?\u1103? \u1087?\u1088?\u1086?\u1075?\u1088?\u1072?\u1084?\u1084?\u1072? \u1076?\u1083?\u1103? \u1088?\u1072?\u1073?\u1086?\u1090?\u1099? \u1089? \u1087?\u1077?\u1088?\u1077?\u1095?\u1080?\u1089?\u1083?\u1077?\u1085?\u1080?\u1077?\u1084?.\par
    public static void main(String[] args) \{\par
        int[] val = new int[7];\par
        for (int i=0; i<7; i++)\par
            val[i] = 17 + i;\par
        Enum e = new Enum(val);\par
        while (e.hasMoreElements()) \{\par
            System.out.println( ((Integer)(e.nextElement()))\par
                                .intValue() );\par
        \}\par
    \}\par
\}\par
